{
  "name": "CustomRequest",
  "lowCaseName": "customrequest",
  "description": "Use friendly URLs everywhere",
  "author": "Thomas Jakobi",
  "version": "1.3.7",
  "package": {
    "menus": [
      {
        "text": "customrequest.menu_home",
        "description": "customrequest.menu_home_desc",
        "action": "home",
        "parent": "components"
      }
    ],
    "elements": {
      "plugins": [
        {
          "name": "CustomRequest",
          "description": "Use friendly URLs everywhere",
          "file": "customrequest.plugin.php",
          "events": [
            "OnDocFormDelete",
            "OnDocFormSave",
            "OnDocPublished",
            "OnDocUnPublished",
            "OnPageNotFound",
            "OnSiteRefresh"
          ]
        }
      ]
    },
    "systemSettings": [
      {
        "key": "debug",
        "type": "combo-boolean",
        "area": "system",
        "value": "0"
      }
    ]
  },
  "database": {
    "tables": [
      "CustomrequestConfigs"
    ]
  },
  "dependencies": [
    {
      "name": "php",
      "version": ">=7.2"
    },
    {
      "name": "modx",
      "version": ">=2.6"
    }
  ],
  "build": {
    "readme": "docs/readme.md",
    "license": "docs/license.md",
    "changelog": "docs/changelog.md",
    "setupOptions": {
      "source": "setup.options.php"
    },
    "resolver": {
      "after": [
        "resolve.synctables.php",
        "resolve.treehillstudio.php"
      ]
    }
  }
}
